{
  "name": "MarkMed DarkPDF",
  "description": "MarkMed Chrome Extension to turn darker the PDF browser viewer",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "webNavigation",
    "webRequest",
    "scripting",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "file:///*.pdf",
    "*://*/*.pdf"
  ],
  "action": {
    "default_title": "MarkMed DarkPDF",
    "default_icon": "./assets/mkmPDF.png"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*.pdf",
        "*://*/*.pdf",
        "file:///*.pdf?*",
        "*://*/*.pdf?*"
      ],
      "css": ["./styles/modalStyles.css"],
      "js": [
        
        "./scripts/main.js",
        "./scripts/modal.js"
      ],
      "run_at": "document_idle"
    }
  ]
}